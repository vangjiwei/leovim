{
    "coc.preferences": {
        "useQuickfixForLocations": false,
        "colorSupport": false,
        "extensionUpdateCheck": "weekly",
        "messageLevel": "error",
        "listOfWorkspaceEdit": "location",
        "hoverTarget": "float",
        "enableFloatHighlight": true
    },
    "coc.source.file.triggerCharacters": ["/"],
    "coc.source.buffer.disableSyntaxes": [],
    "hover.floatConfig": {
        "border": true,
        "highlight": "Normal",
        "maxWidth": 100,
        "focusable": false
    },
    "list": {
        "nextKeymap":         "<C-j>",
        "previousKeymap":     "<C-k>",
        "extendedSearchMode": true,
        "insertMappings": {
          "<C-n>": "",
          "<C-p>": "",
          "<C-j>": "normal:j",
          "<C-k>": "normal:k"
        },
        "normalMappings": {
          "<C-n>": "",
          "<C-p>": "",
          "<C-j>": "normal:next",
          "<C-k>": "normal:previous"
        }
    },
    "suggest.enablePreselect": true,
    "suggest.noselect": true,
    "suggest.defaultSortMethod": "none",
    "suggest.snippetIndicator": "",
    "suggest.languageSourcePriority": 99,
    "suggest.triggerCompletionWait": 50,
    "suggest.acceptSuggestionOnCommitCharacter": true,
    "suggest.floatEnable": true,
    "suggest.floatConfig": {
        "winblend": 8
    },
    "diagnostic.enable": true,
    "diagnostic.displayByAle": false,
    "diagnostic.errorSign": "x",
    "diagnostic.warningSign": "!",
    "diagnostic.infoSign": ">",
    "diagnostic.hintSign": "-",
    "diagnostic.messageTarget": "float",
    "diagnostic.floatConfig": {
        "border": true,
        "highlight": "Normal",
        "maxWidth": 100,
        "focusable": false
    },
    "diagnostic.signPriority": 20,
    "diagnostic.enableHighlightLineNumber": false,
    "diagnostic.locationlistUpdate": false,
    "signature.target": "float",
    "signature.floatConfig": {
        "border": true,
        "highlight": "Normal",
        "maxWidth": 100,
        "focusable": false
    },

    "dialog.floatHighlight": "Normal",
    "dialog.floatBorderHighlight": "Normal",

    "git.enableGutters":   false,
    "git.realtimeGutters": false,

    "languageserver": {
        "dockerfile": {
            "command": "docker-langserver",
            "filetypes": ["dockerfile"],
            "args": ["--stdio"]
        }
    },
    "tsserver.trace.server": "off",
    "vimlsp.trace.server": "off",
    "json.trace.server": "off",
    "xml.trace.server": "off",
    "html.trace.server": "off",
    "css.trace.server": "off",
    "java.trace.server": "off",
    "java.enabled": true,
    "java.codeGeneration.useBlocks": true,
    "java.import.maven.enabled": true,
    "java.maven.downloadSources": false,
    "python.analysis.diagnosticMode": "openFilesOnly",
    "python.linting.pylintEnabled": true,
    "python.linting.flake8Enabled": true,
    "go.trace.server": "off",
    "go.goplsPath": "gopls",
    "go.goplsUseDaemon": false,
    "go.goplsOptions": { 
        "usePlaceholders": true, 
        "semanticTokens": true 
    },
    "rust-analyzer.trace.server": "off",
    "rust-analyzer.server.path": "rust-analyzer",
    "rust-analyzer.inlayHints.enable": false,
    "eslint.packageManager": "yarn",
    "eslint.run": "onSave",
    "yaml.trace.server": "off",
    "yaml.validate": true,
    "yaml.format.singleQuote": true,
    "yaml.completion": true,

    "snippets.ultisnips.enable": true,
    "snippets.loadFromExtensions": true,
    "snippets.enableStatusItem": false,
    "snippets.snipmate.enable": false,
    "snippets.shortcut": "SP",
    "snippets.extends": {
        "cpp": ["c"],
        "typescript": ["javascript"]
    },

    "explorer.keyMappings.global":{
        ",":       "actionMenu",
        "<tab>":   "toggleSelection",
        "<bs>":    "gotoParent",
        "<cr>":    "open",
        "r":       "rename",
        "t":       "open:tab",
        "v":       "open:vsplit",
        "x":       "open:split",
        "h":       "collapse",
        "l":       "expand",
        "<space>": "expandOrCollapse",
        "<C-j>":   ["normal:j"],
        "<C-k>":   ["normal:k"],
        "J":       ["toggleSelection", "normal:j"],
        "K":       ["toggleSelection", "normal:k"],
        "c":       "copyFilepath",
        "C":       "copyFilename",
        "y":       "copyFile",
        "m":       "cutFile",
        "p":       "pasteFile",
        "d":       "delete",
        "D":       "deleteForever",
        "a":       "addFile",
        "A":       "addDirectory",
        "<F1>":    "help",
        "H":       "toggleHidden",
        "<F5>":    "refresh",
        "q":       "quit",
        "X":       "systemExecute",
        "f":       "search",
        "F":       "searchRecursive",
        "gl":      "expandRecursive",
        "gh":      "collapseRecursive",
        "gp":      "diagnosticPrev",
        "gn":      "diagnosticNext",
        "gd":      "listDrive",
        ">>":      "gitStage",
        "<<":      "gitUnstage"
    }
}
